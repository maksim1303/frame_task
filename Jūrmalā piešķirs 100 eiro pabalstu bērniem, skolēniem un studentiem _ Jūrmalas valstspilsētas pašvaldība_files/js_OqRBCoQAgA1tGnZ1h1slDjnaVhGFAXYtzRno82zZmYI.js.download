(function ($, Drupal) {
    Drupal.behaviors.tvp_contacts = {
        attach: function (context, settings) {

            let isMobile = (window.innerWidth < 768);

            function manageBreadCrumbs(optionTitle, optionId) {
                var textForBreadCrumbs = optionTitle[0].innerHTML;
                var allItems = [];

                $('#structure-breadcrumbs').append("<div class='structure-breadcrumb-item' id='" + optionId + "'>" + textForBreadCrumbs + "</div>");

                $.each($('.structure-breadcrumb-item'), function () {
                    allItems.push(this);

                    // With each breadcrumb item we will track previous item margin-left
                    // and add +20 each time. We need to remove -20 for the first item element (no margin-left)
                    $(this).css('marginLeft', (allItems.length * 20) - 20);
                });
            }

            $(document).delegate('.structure-breadcrumb-item', 'click', function (event) {
                event.stopPropagation();

                $(this).nextAll(".structure-breadcrumb-item").remove();

                var getSelectedItemUl = $('.department-content-block > .structure-menu > ul').find('ul#' + $(this).attr('id'));
                if (getSelectedItemUl[0]){
                    cloneMenu(getSelectedItemUl, false, $(this).attr('id'));
                }
            });

            function cloneMenu(menu_element, init, breadcrumb_id) {
                var menuClone = $(menu_element).clone();

                if (init) {
                    $('#structure-menu-mob').html(menuClone);
                } else {
                    $('#structure-menu-mob').html('<div class="structure-menu"><ul>' + menuClone.html() + '</ul></div>');
                }

                if (breadcrumb_id) {
                    $('.department-info').html($('.department-info-' + breadcrumb_id).html());
                    $('.department-boss').replaceWith($('.department-info').css('display', 'block'));
                }

                $('#structure-menu-mob li').on('click', function (event) {
                    event.stopPropagation();

                    var cloneParentUl = $(this).find('ul');
                    var grabTitleForBreadCrumbs = $(this).children('.anchor');
                    var grabIdForBreadCrumbs = $(this).attr('id');

                    if (cloneParentUl.length > 0) {
                        cloneMenu(cloneParentUl);
                    } else {
                        $('.department-content-block > #structure-menu-mob').html('');
                    }

                    var infoId = $(this).attr('id');
                    $('.department-info').html($('.department-info-' + infoId).html());

                    $('.department-boss').replaceWith($('.department-info').css('display', 'block'));
                    $('.department-content-block').removeClass("row");
                    manageBreadCrumbs(grabTitleForBreadCrumbs, grabIdForBreadCrumbs);
                });
            }
            jQuery(document).delegate('.structure-menu > ul > li.main-tree-item > span.anchor', 'click', function () {
                //when clicked on main menu item, if it has no child, no department info shoud be seen
                jQuery('.department-info').html('');
            });

            jQuery(document).delegate('.structure-menu > ul li.tree-item > span.anchor', 'click', function () {
                var infoId = $(this).parent('li').attr('id');

                jQuery('.department-info').html('');

                if (infoId) {
                    if (!isMobile) {
                        $('[class^="department-info-"]').css({
                            display: 'none'
                        });
                    }

                    $('.department-info').html($('.department-info-' + infoId).html());

                    if (!isMobile) {
                        $('.department-info-' + infoId).css({
                            display: 'block'
                        });
                    }
                }
            });
            let initaialState = $('.department-boss').clone()
            $('.type-department #header-info .display-4').on('click', function(){
                if(isMobile){
                    cloneMenu('#parent-level', false);
                    $('.department-info').html( initaialState )
                    Drupal.behaviors.spamspan.attach()
                    $('#structure-breadcrumbs').html('')
                }

            })

            if (isMobile) {
                $('#structure-menu-mob').css('display', 'block');
                cloneMenu('.structure-menu', true);
                $('.department-content-block > .structure-menu').hide();
            }

            if (!isMobile) {
                $(".structure-menu li.tree-item span.anchor").on('click', function (event) {
                    let element = $(this).parent('li.tree-item');
                    if (!element.hasClass("active")) {
                        var infoId = element.attr('id');
                        $('.department-info').html($('.department-info-' + infoId).html());

                        var depHeight = $('.department-info-' + infoId).height();
                        $(".structure-menu ul").each(function () {
                            if ($(this).css('display') == 'block') {
                                if ($(this).height() < depHeight) {
                                    $(this).css('height', depHeight);
                                }
                            }
                        });
                    }
                });

                $(document).delegate('.structure-menu li.tree-item span.anchor', 'click', function (event) {
                    event.stopPropagation();

                    if (!isMobile) {
                        $('.scrollbar-inner').scrollbar();
                    }

                    var totalWidth = 0;
                    var parentHeight = $('.structure-menu ul#parent-level').height();
                    var maxHeight = 0;
                    var clickedLiElement = $(this).parent('li.tree-item');

                    if ($(clickedLiElement).children('ul')) {
                        var childrenList = clickedLiElement.children('ul');
                        var childrenListHeight = childrenList.height();

                        if (!clickedLiElement.hasClass('active')) {
                            if (childrenListHeight > parentHeight && childrenListHeight > maxHeight) {
                                maxHeight = childrenListHeight;
                            }
                        }
                    }

                    $(clickedLiElement).addClass('active');

                    $('li', '.structure-menu')
                        .not(clickedLiElement
                        ).not(clickedLiElement.parents('li', '.structure-menu'))
                        .removeClass('active');


                    $(".structure-menu ul").each(function () {
                        var isVisible = $(this).css('display');
                        if (isVisible == 'block') {
                            totalWidth += $(this).width();
                        }
                    });

                    $('.department-info').css('left', totalWidth + 30);

                    if (!isMobile) {
                        var infoId = $(this).parent('li').attr('id');
                        var $data_element = $('.department-info-' + infoId);
                        var data_offset = $data_element.offset();
                        var data_position = $data_element.position();
                        var department_info_offset = $('.department-info').offset();

                        $data_element.css({
                            left: department_info_offset.left - data_offset.left + data_position.left
