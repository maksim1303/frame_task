              $('#block-menucustomblock .navbar .navbar-toggler').click()
              setUpDesktopNav()

            }

          }
          setUpTabletNav()
          setUpMobileAndTAbletNav()
          setUpDesktopAndTAbletNav()

        }

        function desktopMediaSetup(match){
          if(match.matches){

            menuMobile = false
            menuTablet = false

            if($('html').hasClass('font-size-1-25x') || $('html').hasClass('font-size-1-50x')){
              menuTablet = true
            }

            setUpMobileAndTAbletNav()
            setUpTabletNav()

