        // # this Part is resposible for handling responsivnes #
        // ###################################


        function mobileMediaSetup(match){

          menuTablet = false
          if(match.matches){
            setUpMobileAndTAbletNav()
            setUpDesktopAndTAbletNav()
            setUpTabletNav()
            leaveActiveWindow()


            $('.access-container').css('display','none')
